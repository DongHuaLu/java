<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>Person</servlet-name>
    <servlet-class>com.dolph.servlet.PersonServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PersonJson</servlet-name>
    <servlet-class>com.dolph.servlet.PersonJsonServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Person</servlet-name>
    <url-pattern>/person.do</url-pattern>
  </servlet-mapping>	
  <servlet-mapping>
    <servlet-name>PersonJson</servlet-name>
    <url-pattern>/personJson.do</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
